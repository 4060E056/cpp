#include<stdio.h> /* 導入標準輸入輸出*/
 
int main() /*宣告整數主要函式*/
{
    int i,j,k;  /*宣告整數變數i.j.k*/
    printf("\n"); /*斷行*/
    for(i=1;i<5;i++) {  /*for迴圈i=1 i<5 i++*/
        for(j=1;j<5;j++) { /*for迴圈j=1 j<5 j++*/  
            for (k=1;k<5;k++) {  /*for迴圈k=1 k<5 k++*/
                if (i!=k&&i!=j&&j!=k) {  /*if判斷 i不等於k 且 i不等於j 且 j不等於k*/
                    printf("%d,%d,%d\n",i,j,k); /*印出i j k 的數值*/
                }
            }
        }
    }
}
